{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "sass src/scss/:dist/css/ --style compressed",
    "build:js": "babel src/js -d dist/js --presets=@babel/preset-env",
    "watch:css": "sass --watch src/scss/:dist/css/ --style compressed",
    "watch:js": "babel src/js -d dist/js --presets=@babel/preset-env --watch",
    "build": "npm run build:css && npm run build:js && npm run copy:html",
    "watch": "npm-run-all --parallel watch:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.84.0"
  }
}
